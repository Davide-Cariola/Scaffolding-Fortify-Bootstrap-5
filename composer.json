{
    "name": "davidecariola/scaffolding-fortify-bootstrap",
    "description": "Automatically creates a Laravel 8 scaffolding with Laravel/Fortify and Bootstrap 5",
    "keywords": ["laravel", "fortify", "bootstrap", "auth"],
    "require": {
        "illuminate/support": "~7|~8",
        "php": "^7.3|^8.0",
        "laravel/fortify": "2.x-dev",
        "laravel/socialite": "^5.0"
    },
    "authors": [
        {
            "name": "Davide Cariola",
            "email": "davidecariola90@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DavideCariola\\ScaffoldingFortifyBootstrap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DavideCariola\\ScaffoldingFortifyBootstrap\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DavideCariola\\ScaffoldingFortifyBootstrap\\ScaffoldingFortifyBootstrapServiceProvider"
            ],
            "aliases": {
                "ScaffoldingFortifyBootstrap": "DavideCariola\\ScaffoldingFortifyBootstrap\\Facades\\ScaffoldingFortifyBootstrap"
            }
        }
    },
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev"
}
